{
	"printWidth": 100,
	"trailingComma": "es5",
	"tabWidth": 4,
	"useTabs": true,
	"endOfLine": "auto",
	"requirePragma": false,
	"insertPragma": false,
	"htmlWhitespaceSensitivity": "ignore",
	"vueIndentScriptAndStyle": true,
	"plugins": [
		"prettier-plugin-svelte"
	],
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	]
}