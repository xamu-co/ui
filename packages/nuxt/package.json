{
  "name": "@open-xamu-co/ui-nuxt",
  "version": "3.0.0",
  "description": "Nuxt 3 module for the xamu project",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xamu-co/ui.git"
  },
  "keywords": [
    "vue",
    "component",
    "system"
  ],
  "author": "@xamu-co",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xamu-co/ui/issues"
  },
  "homepage": "https://github.com/xamu-co/ui/packages/vue#readme",
  "type": "module",
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18",
    "yarn": ">=1.22.4"
  },
  "scripts": {
    "build": "yarn build:prepack && nuxt-module-build build",
    "build:prepack": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@nuxt/image": "^1.10.0",
    "@nuxt/kit": "^3.16.1",
    "@open-xamu-co/ui-common-enums": "^3.0.0",
    "@open-xamu-co/ui-common-helpers": "^3.0.0",
    "@open-xamu-co/ui-components-vue": "^3.0.0",
    "nuxt-countries-api": "^1.0.3",
    "sweetalert2": "^11.11.1"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.3.2",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.16.1",
    "@nuxt/test-utils": "^3.17.2",
    "@open-xamu-co/ui-common-types": "^3.0.0",
    "@open-xamu-co/ui-styles": "^4.0.0",
    "@types/lodash-es": "^4.14.192",
    "lodash-es": "^4.17.21",
    "nuxi": "^3.23.1",
    "nuxt": "^3.16.1",
    "vitest": "^0.33.0"
  }
}
