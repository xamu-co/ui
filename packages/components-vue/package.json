{
  "name": "@open-xamu-co/ui-components-vue",
  "version": "4.0.0-next.4",
  "description": "Vue component system for the xamu project",
  "scripts": {
    "build:entries": "rimraf dist && vite build",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly",
    "build": "yarn build:entries && yarn build:types",
    "dev": "storybook dev",
    "typecheck": "vue-tsc --build",
    "test": "vitest run --coverage",
    "storybook:build": "storybook build",
    "storybook:test": "vitest run --project=storybook --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xamu-co/ui.git"
  },
  "keywords": [
    "vue",
    "component",
    "system"
  ],
  "author": "@xamu-co",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xamu-co/ui/issues"
  },
  "homepage": "https://github.com/xamu-co/ui/packages/vue#readme",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "type": "module",
  "files": [
    "CHANGELOG.md",
    "README.md",
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./plugin": {
      "types": "./dist/plugin.d.ts",
      "import": "./dist/plugin.js",
      "require": "./dist/plugin.cjs",
      "default": "./dist/plugin.js"
    },
    "./theme": {
      "types": "./dist/composables/theme.d.ts",
      "import": "./dist/theme.js",
      "require": "./dist/theme.cjs",
      "default": "./dist/theme.js"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-common-types": "^6.4.0",
    "@open-xamu-co/ui-common-enums": "^4.0.0-next.1",
    "@open-xamu-co/ui-common-helpers": "^4.0.0-next.2",
    "lodash-es": "^4.17.21",
    "sweetalert2": "^11.11.1",
    "ts-md5": "^1.3.1",
    "validator": "^13.11.0",
    "vue-color": "^3.3.1"
  },
  "devDependencies": {
    "@open-xamu-co/ui-common-types": "^4.0.0-next.3",
    "@open-xamu-co/ui-styles": "^5.0.0-next.3",
    "@playwright/test": "^1.57.0",
    "@storybook/addon-docs": "^10.1.9",
    "@storybook/addon-links": "^10.1.9",
    "@storybook/addon-onboarding": "^10.1.9",
    "@storybook/addon-vitest": "^10.1.9",
    "@storybook/vue3-vite": "^10.1.9",
    "@types/jsdom": "^27.0.0",
    "@types/lodash-es": "^4.14.192",
    "@types/validator": "^13.11.1",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vitest/browser-playwright": "^4.0.14",
    "@vitest/coverage-v8": "^4.0.14",
    "jsdom": "^27.2.0",
    "msw": "^2.12.4",
    "playwright": "^1.57.0",
    "storybook": "^10.1.9"
  },
  "peerDependencies": {
    "vue": "^3.5.25",
    "vue-router": "^4.6.4"
  },
  "peerDependenciesMeta": {
    "vue-router": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=20.19.3",
    "yarn": ">=1.22.4"
  }
}
