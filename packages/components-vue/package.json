{
  "name": "@open-xamu-co/ui-components-vue",
  "version": "3.1.0-next.8",
  "description": "Vue component system for the xamu project",
  "scripts": {
    "build:entries": "rimraf dist && vite build",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly",
    "build": "yarn build:entries && yarn build:types",
    "dev": "storybook dev",
    "storybook:build": "storybook build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xamu-co/ui.git"
  },
  "keywords": [
    "vue",
    "component",
    "system"
  ],
  "author": "@xamu-co",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xamu-co/ui/issues"
  },
  "homepage": "https://github.com/xamu-co/ui/packages/vue#readme",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "files": [
    "CHANGELOG.md",
    "README.md",
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.mjs"
    },
    "./plugin": {
      "types": "./dist/plugin.d.ts",
      "import": "./dist/plugin.mjs",
      "require": "./dist/plugin.js",
      "default": "./dist/plugin.mjs"
    },
    "./theme": {
      "types": "./dist/composables/theme.d.ts",
      "import": "./dist/theme.mjs",
      "require": "./dist/theme.js",
      "default": "./dist/theme.mjs"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-common-types": "^6.4.0",
    "@open-xamu-co/ui-common-enums": "^3.1.0-next.1",
    "@open-xamu-co/ui-common-helpers": "^3.0.2-next.4",
    "lodash-es": "^4.17.21",
    "sweetalert2": "^11.11.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "1.4.0",
    "@open-xamu-co/ui-common-types": "^3.1.0-next.2",
    "@open-xamu-co/ui-styles": "^4.0.1-next.2",
    "@storybook/addon-essentials": "^8.1.1",
    "@storybook/addon-interactions": "^8.1.1",
    "@storybook/addon-links": "^8.1.1",
    "@storybook/blocks": "^8.1.1",
    "@storybook/test": "^8.1.1",
    "@storybook/vue3": "^8.1.1",
    "@storybook/vue3-vite": "^8.1.1",
    "@types/lodash-es": "^4.14.192",
    "@types/validator": "^13.11.1",
    "@vitejs/plugin-vue": "^5.2.0",
    "storybook": "^8.1.1",
    "ts-md5": "^1.3.1",
    "validator": "^13.11.0",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1"
  },
  "peerDependencies": {
    "vue": "^3.5.17",
    "vue-router": "^4.5.1"
  },
  "peerDependenciesMeta": {
    "vue-router": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=20.19.3",
    "yarn": ">=1.22.4"
  }
}
