{
	"name": "@open-xamu-co/ui-components-vue",
	"version": "3.0.0-next.7",
	"description": "Vue component system for the xamu project",
	"scripts": {
		"i": "yarn --frozen-lockfile",
		"build:entries": "rimraf dist && vite build",
		"build:types": "vue-tsc --declaration --emitDeclarationOnly",
		"build": "yarn build:entries && yarn build:types",
		"dev": "vite dev"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/xamu-co/ui.git"
	},
	"keywords": [
		"vue",
		"component",
		"system"
	],
	"author": "@xamu-co",
	"publishConfig": {
		"access": "public"
	},
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/xamu-co/ui/issues"
	},
	"homepage": "https://github.com/xamu-co/ui/packages/vue#readme",
	"type": "module",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"typings": "dist/index.d.ts",
	"module": "dist/index.js",
	"files": [
		"CHANGELOG.md",
		"README.md",
		"dist"
	],
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./plugin": {
			"types": "./dist/plugin.d.ts",
			"import": "./dist/plugin.js",
			"require": "./dist/plugin.cjs"
		},
		"./theme": {
			"types": "./dist/composables/theme.d.ts",
			"import": "./dist/theme.js",
			"require": "./dist/theme.cjs"
		}
	},
	"dependencies": {
		"@fortawesome/fontawesome-common-types": "^6.4.0",
		"@open-xamu-co/ui-common-enums": "^3.0.0-next.1",
		"@open-xamu-co/ui-common-helpers": "^3.0.0-next.4",
		"@open-xamu-co/ui-common-types": "^3.0.0-next.5",
		"@types/lodash": "^4.14.192",
		"@types/validator": "^13.11.1",
		"js-md5": "^0.8.3",
		"lodash": "^4.17.21",
		"nanoid": "^4.0.2",
		"sweetalert2": "^11.7.5",
		"validator": "^13.11.0"
	},
	"devDependencies": {
		"@open-xamu-co/ui-styles": "^3.0.0-next.4",
		"@types/node": "^18.17.17",
		"@vitejs/plugin-vue": "^4.2.1",
		"vite": "^5.0.10",
		"vue": "^3.4.18",
		"vue-router": "^4.2.5",
		"vue-tsc": "^1.8.27"
	},
	"peerDependencies": {
		"vue": "^3.4.18",
		"vue-router": "^4.2.5"
	},
	"peerDependenciesMeta": {
		"vue-router": {
			"optional": true
		}
	},
	"engines": {
		"node": ">=18",
		"yarn": ">=1.22.4"
	}
}