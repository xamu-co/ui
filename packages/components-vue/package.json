{
  "name": "@open-xamu-co/ui-components-vue",
  "version": "0.0.1",
  "description": "Vue component system for the xamu project",
  "scripts": {
    "build:entries": "rimraf dist && vite build",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly",
    "build": "yarn build:entries && yarn build:types",
    "dev": "storybook dev",
    "typecheck": "vue-tsc --build",
    "test": "vitest run --coverage",
    "storybook:build": "storybook build",
    "storybook:test": "vitest run --project=storybook --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xamu-co/ui.git"
  },
  "keywords": [
    "vue",
    "component",
    "system"
  ],
  "author": "@xamu-co",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xamu-co/ui/issues"
  },
  "homepage": "https://github.com/xamu-co/ui/packages/vue#readme",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "type": "module",
  "files": [
    "CHANGELOG.md",
    "README.md",
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.mjs"
    },
    "./plugin": {
      "types": "./dist/plugin.d.ts",
      "import": "./dist/plugin.mjs",
      "require": "./dist/plugin.js",
      "default": "./dist/plugin.mjs"
    },
    "./theme": {
      "types": "./dist/composables/theme.d.ts",
      "import": "./dist/theme.mjs",
      "require": "./dist/theme.js",
      "default": "./dist/theme.mjs"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-common-types": "^6.4.0",
    "@open-xamu-co/ui-common-enums": "link:../common-enums",
    "@open-xamu-co/ui-common-helpers": "link:../common-helpers",
    "lodash-es": "^4.17.21",
    "sweetalert2": "^11.11.1",
    "vue-color": "^3.3.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.3",
    "@open-xamu-co/ui-common-types": "link:../common-types",
    "@open-xamu-co/ui-styles": "link:../styles",
    "@playwright/test": "^1.57.0",
    "@storybook/addon-docs": "^10.1.5",
    "@storybook/addon-links": "^10.1.5",
    "@storybook/addon-onboarding": "^10.1.5",
    "@storybook/addon-vitest": "^10.1.5",
    "@storybook/vue3-vite": "^10.1.5",
    "@types/jsdom": "^27.0.0",
    "@types/lodash-es": "^4.14.192",
    "@types/validator": "^13.11.1",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vitest/browser-playwright": "^4.0.14",
    "@vitest/coverage-v8": "^4.0.14",
    "jsdom": "^27.2.0",
    "msw": "^2.12.4",
    "storybook": "^10.1.5",
    "ts-md5": "^1.3.1",
    "validator": "^13.11.0",
    "vitest": "^4.0.14",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1",
    "vue-tsc": "^3.1.5"
  },
  "peerDependencies": {
    "vue": "^3.5.17",
    "vue-router": "^4.5.1"
  },
  "peerDependenciesMeta": {
    "vue-router": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=20.19.3",
    "yarn": ">=1.22.4"
  }
}