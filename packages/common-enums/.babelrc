{
   "presets": [
      "@babel/preset-typescript",
      [
         "@babel/preset-env",
         {
            "useBuiltIns": "usage",
            "corejs": 3,
            "targets": "defaults"
         }
      ]
   ],
   "plugins": [
      [
         "replace-import-extension",
         {
            "extMapping": {
               ".js": ".cjs"
            }
         }
      ]
   ]
}